<h1><%= @artist.name %></h1>

<h2> Songs </h2>
<ul>
  <% @artist.songs.each do |song| %>
  <li>
    <h2><a href="/songs/<%=song.slug%>"><%= song.name %></a></h2>
  </li>
  <% end %>
</ul>

<h2> Genres </h2>
<ul>
  <% @artist.genres.each do |genre| %>
  <li>
    <h2><a href="/genres/<%=genre.slug%>"><%=genre.name%></a></h2>
  </li>
  <% end %>
</ul>
